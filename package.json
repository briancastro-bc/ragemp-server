  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "scripts": {
    "build:cef": "cd ./ui && pnpm build",
    "build": "concurrently \"pnpm build:vite\" \"rollup -c ./scripts/rollup.config.js\"",
    "watch": "cd ./dist && nodemon -w packages -w 'client_packages/*' --exec ragemp-server.exe -e *",
    "watch:build": "concurrently \"pnpm build:cef\" \"rollup -w -c ./scripts/rollup.config.js\" \"pnpm nodemon\"",
    "build:production": "rollup -c ./scripts/rollup.config.js --environment NODE_ENV:production",
    "dev": "vite",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "colorette": "^2.0.16",
    "dotenv": "^16.4.5",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@ragempcommunity/types-client": "^2.1.8",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^4.0.0",
    "@swc/core": "^1.2.118",
    "@swc/helpers": "^0.3.2",
    "@types/node": "^17.0.23",
    "builtin-modules": "^3.2.0",
    "concurrently": "^9.0.1",
    "fs-jetpack": "^4.2.0",
    "globals": "^15.9.0",
    "nodemon": "^3.1.4",
    "prettier": "^2.5.0",
    "rollup": "^2.60.1",
    "rollup-plugin-swc3": "^0.1.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-tsconfig-paths": "^1.0.15",
    "rollup-plugin-typescript2": "^0.31.1",
    "root": "github:briancastro-bc/ragemp-types",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1"
  }
}