{
  "exclude": [
    "node_modules", 
    "dist"
  ],
  "compileOnSave": true,
  "compilerOptions": {
    "incremental": true,
    "target": "ESNext",
    "lib": [
      "ES2016",
      "ESNext",
      "DOM",
      "WebWorker",
      "Decorators"
    ],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "useDefineForClassFields": true,
    "module": "ESNext",
    "rootDir": "./",
    "moduleResolution": "node",
    "baseUrl": "./",
    "resolveJsonModule": true,
    "allowJs": true,
    "checkJs": true,
    "declaration": false,
    "declarationMap": false,
    "sourceMap": false,
    "outDir": "./dist",
    "removeComments": true,
    "importHelpers": true,
    "noEmitHelpers": true,
    "preserveConstEnums": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": false
  },
  "references": [
    
  ]
}
